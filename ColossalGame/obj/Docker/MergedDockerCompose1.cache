services:
  colossalgame:
    build:
      context: C:\Users\jlsan\Source\Repos\UB-CSE\course-project-colossal-crab-comrades\ColossalGame
      dockerfile: ColossalGame/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ColossalGame
      target: base
    container_name: ColossalGame
    depends_on:
    - db
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: colossalgame:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/ColossalGame.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
    - db
    ports:
    - 80/tcp
    - 443/tcp
    tty: true
    volumes:
    - C:\Users\jlsan\Source\Repos\UB-CSE\course-project-colossal-crab-comrades\ColossalGame\ColossalGame:/app:rw
    - C:\Users\jlsan\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\jlsan\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\jlsan\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\jlsan\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\jlsan\Source\Repos\UB-CSE\course-project-colossal-crab-comrades\ColossalGame:/src:rw
  db:
    container_name: mongo
    image: mongo
version: '3.4'